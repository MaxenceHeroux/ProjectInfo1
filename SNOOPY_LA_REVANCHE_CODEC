#include <stdio.h>
#include <stdlib.h> //Bibliothèque pour importer le clear console
#include "unistd.h" //Bibliothèque pour importer sleep()
#include "Bibliotheque SNOOPY.h"

main(){
    int i=0;
    for(i=0;i<120;i++){
        sleep(1);
        if(i<30){
            int l=0;
            for(l;l<i;l++){
                printf(" ");
            }
            printf("-");
            system("cls"); //clear console
        }

    }

    return 0; //return 0
}
